{
  "name": "monorepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:nestjs": "pnpm run --filter nestjs build",
    "clean:dist": "pnpm rimraf \"**/dist\" --glob",
    "clean:node_modules": "pnpm dlx rimraf \"**/node_modules\" --glob",
    "clean:turbo": "pnpm dlx rimraf \"**/.turbo\" --glob",
    "db:generate": "pnpm run --filter nestjs prisma:generate",
    "db:push": "pnpm run --filter nestjs db:push",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lint:fix": "biome check --write",
    "prepare": "husky",
    "start:nestjs": "pnpm run --filter nestjs start"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "pnpm lint:fix",
    "**/package.json": "sort-package-json"
  },
  "dependencies": {
    "@nestjs/mapped-types": "*",
    "@radix-ui/react-avatar": "1.1.10",
    "@radix-ui/react-collapsible": "1.1.11",
    "@radix-ui/react-dialog": "1.1.14",
    "@radix-ui/react-dropdown-menu": "2.1.15",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-popover": "1.1.14",
    "@radix-ui/react-progress": "1.1.7",
    "@radix-ui/react-scroll-area": "1.2.9",
    "@radix-ui/react-select": "2.2.5",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slider": "1.3.5",
    "@radix-ui/react-tabs": "1.1.12",
    "@radix-ui/react-tooltip": "1.2.7",
    "date-fns": "4.1.0",
    "input-otp": "1.4.2",
    "react-day-picker": "9.8.0",
    "react-dropzone": "14.3.8",
    "react-hook-form": "7.59.0",
    "sonner": "2.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0",
    "@workspace/typescript-config": "workspace:*",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "rimraf": "6.0.1",
    "sort-package-json": "3.2.1",
    "turbo": "2.5.4",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184",
  "engines": {
    "node": ">=20"
  }
}
